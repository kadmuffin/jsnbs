{
  "name": "jsnbs",
  "version": "0.1.0",
  "description": "A Port of pynbs by vberlier for the web!",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts, .tsx\" --source-maps inline",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kadmuffin/jsnbs.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kadmuffin/jsnbs/issues"
  },
  "homepage": "https://github.com/kadmuffin/jsnbs#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.5",
    "babel-jest": "^26.1.0",
    "jest": "^26.1.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@types/jbinary": "0.0.30",
    "core-js": "^3.6.5",
    "jbinary": "^2.1.3",
    "regenerator-runtime": "^0.13.5"
  }
}

{
  "name": "jsnbs",
  "version": "0.1.0",
  "description": "A Port of pynbs by vberlier for the web!",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "unpkg": "dist/index.umd.min.js",
  "jsdelivr": "dist/index.umd.min.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "test": "npm run test:lint && npm run test:unit && npm run test:clean",
    "test:lint": "prettier **/*.{ts,js,json} --check",
    "test:unit": "jest --runInBand --detectOpenHandles",
    "test:clean": "shx rm write_test.nbs",
    "lint": "prettier **/*.{ts,js,json} --write",
    "build": "npm run clean && npm run build:bili",
    "build:bili": "bili",
    "clean": "shx rm -rf dist types",
    "prepublishOnly": "npm run build && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kadmuffin/jsnbs.git"
  },
  "author": "KadMuffin <kadmuffin@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kadmuffin/jsnbs/issues"
  },
  "homepage": "https://github.com/kadmuffin/jsnbs#readme",
  "devDependencies": {
    "@types/jbinary": "0.0.30",
    "@types/jest": "^26.0.5",
    "bili": "^5.0.5",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "rollup-plugin-typescript2": "^0.27.1",
    "shx": "^0.3.2",
    "ts-jest": "^26.1.3",
    "tslib": "^2.0.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "jbinary": "^2.1.3"
  },
  "keywords": [
    "noteblock",
    "jsnbs",
    "nbs"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test",
      "pre-push": "npm run lint && npm run test"
    }
  }
}
